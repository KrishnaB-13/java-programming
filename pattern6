/*
1
01
101
0101
10101

*/
public class Pattern {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
    
        for(int i=1;i<=4;i++)
        {
            for(int j=1;j<=i;j++)
            {    
                if((i+j)%2==0)
					System.out.print("1");
				else
					System.out.print("0");
            }
        System.out.println();
        }
    }
}
