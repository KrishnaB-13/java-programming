public class Pattern {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
    
        for(int i=1;i<=5;i++)
        {
            for(int j=1;j<=i;j++)
            {    
				System.out.print(i+ " ");
            }
        System.out.println("");
        }
    }
}
